{
  "name": "phone-number-input",
  "version": "1.0.8",
  "author": "@lbgm",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "build-only": "vite build",
    "build-lib": "vite build && vue-tsc --emitDeclarationOnly --allowJs && mv dist/src dist/types",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "engines": {
    "node": ">=14.18.3",
    "npm": ">=6.14.15"
  },
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "main": "./dist/phone-number-input.umd.js",
  "module": "./dist/phone-number-input.es.js",
  "exports": {
    ".": {
      "import": "./dist/phone-number-input.es.js",
      "require": "./dist/phone-number-input.umd.js"
    }
  },
  "dependencies": {
    "libphonenumber-js": "^1.10.11",
    "lodash": "^4.17.21",
    "sass": "^1.54.1",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/lodash": "^4.14.182",
    "@types/node": "^16.11.45",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.8",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.1.7",
    "typescript": "~4.7.4",
    "vite": "^3.0.1",
    "vue-tsc": "^0.38.8"
  }
}
